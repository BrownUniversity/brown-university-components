{
  "name": "brown-university-theme",
  "description": "React components for the Brown University Theme",
  "author": "Colin Cummings <colin_cummings@brown.edu>",
  "version": "0.3.10",
  "private": true,
  "homepage": "https://bitbucket.brown.edu/projects/CCUM/repos/brown-university-theme",
  "main": "dist/brown-university-theme.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf webpack/stats",
    "lint": "eslint src",
    "prettier": "prettier \"{src,__{tests,mocks}__}/**/*.+(js|json|css)\"",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "test": "jest --config ./jest/config.js",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:open": "npm run test:coverage && opener ./coverage/lcov-report/index.html",
    "test:watch": "npm test -- --watch",
    "validate": "npm-run-all lint format:check test:coverage",
    "prebuild": "npm-run-all validate clean",
    "build": "webpack --config webpack/config.js",
    "start": "start-storybook -p 8080 -c .storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "dependencies": {
    "polished": "^2.3.0",
    "prop-types": "^15.6.2",
    "react-collapse": "^4.0.3",
    "react-fns": "^1.4.0",
    "react-motion": "^0.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.8",
    "@storybook/addon-knobs": "^4.0.7",
    "@storybook/react": "^4.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.9.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-colinrcummings": "^2.1.0",
    "file-loader": "^2.0.0",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "jest-styled-components": "^6.3.1",
    "npm-run-all": "^4.1.3",
    "opener": "^1.5.1",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-svg-loader": "^2.1.0",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2"
  }
}
