{
  "name": "brown-university-components",
  "description": "Components for building React-based applications at Brown University",
  "author": "Colin Cummings <colin_cummings@brown.edu>",
  "version": "1.7.1",
  "private": true,
  "homepage": "https://github.com/BrownUniversity/brown-university-components",
  "main": "dist/index.commonjs.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src stories",
    "prettier": "prettier '**/*.{js,json,css}'",
    "format:check": "npm run prettier -- --list-different",
    "format:fix": "npm run prettier -- --write",
    "test": "jest --config ./jest/config.js",
    "test:verbose": "npm test -- --verbose",
    "test:coverage": "npm test -- --coverage",
    "test:coverage:open": "npm run test:coverage && opener ./coverage/lcov-report/index.html",
    "test:watch": "npm test -- --watch --verbose false",
    "validate": "npm-run-all lint format:check test:coverage",
    "build:commonjs": "webpack",
    "build:commonjs:analyze": "RUN_WEBPACK_BUNDLE_ANALYZER=true npm run build:commonjs",
    "prebuild:docs": "rimraf docs",
    "build:docs": "storybook build -o docs",
    "prebuild:es": "rimraf dist/es",
    "build:es": "BABEL_ENV=es babel src -d dist/es --copy-files",
    "postbuild:es": "rimraf dist/es/components/__tests__ dist/es/assets/svg dist/es/assets/png",
    "prebuild": "npm run validate",
    "build": "npm-run-all build:commonjs build:es",
    "start": "storybook dev -p 8080 -c .storybook"
  },
  "peerDependencies": {
    "brown-university-styles": "github:BrownUniversity/brown-university-styles",
    "react": "^16.0.0",
    "styled-components": "^4.0.0"
  },
  "dependencies": {
    "polished": "^3.4.0",
    "prop-types": "^15.7.2",
    "react-fns": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addons": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-webpack5": "^7.6.17",
    "@storybook/theming": "^7.6.17",
    "@testing-library/react": "^8.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.21",
    "brown-university-styles": "github:BrownUniversity/brown-university-styles",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-brown": "github:BrownUniversity/eslint-config-brown#semver:^0.19.0",
    "jest": "^26.6.3",
    "jest-dom": "^3.5.0",
    "jest-styled-components": "^6.3.4",
    "npm-run-all": "^4.1.3",
    "opener": "^1.5.2",
    "prettier": "^3.2.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-svg-loader": "^3.0.3",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^2.7.1",
    "storybook": "^7.6.17",
    "styled-components": "^4.4.1",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  }
}
